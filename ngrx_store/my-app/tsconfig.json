{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "target": "es6",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [
    "./node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true
  }
}
